<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="930.0" style="-fx-background-color: #59757a;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AppController">
    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d9d9d900" height="370.0" layoutX="16.0" layoutY="14.0" stroke="#eeeeee" strokeType="INSIDE" width="897.0" />
        <AnchorPane layoutX="30.0" layoutY="69.0" prefHeight="303.0" prefWidth="540.0" style="-fx-background-color: #eeeeee;">
            <children>
                <TableView fx:id="appointmentTable" prefHeight="222.0" prefWidth="868.0">
                    <columns>
                        <TableColumn fx:id="appIdCol" prefWidth="54.0" text="App ID" />
                        <TableColumn fx:id="appTitleCol" prefWidth="90.0" text="Title" />
                        <TableColumn fx:id="appDescCol" prefWidth="120.0" text="Description" />
                        <TableColumn fx:id="appLocationCol" prefWidth="110.0" text="Location" />
                        <TableColumn fx:id="appContactCol" prefWidth="90.0" text="Contact" />
                        <TableColumn fx:id="appTypeCol" prefWidth="72.0" text="Type" />
                        <TableColumn fx:id="appStartCol" prefWidth="81.0" text="Start" />
                        <TableColumn fx:id="appEndCol" prefWidth="81.0" text="End" />
                        <TableColumn fx:id="appNameCol" prefWidth="115.0" text="Name" />
                        <TableColumn fx:id="appUserIdCol" prefWidth="54.0" text="User ID" />
                    </columns>
                </TableView>
                <Button fx:id="newAppBtn" layoutX="394.0" layoutY="239.0" mnemonicParsing="false" onAction="#onNewAppClick" prefHeight="27.0" prefWidth="81.0" text="NEW">
                    <font>
                        <Font name="Leelawadee UI" size="15.0" />
                    </font>
                </Button>
                <Button fx:id="updateAppBtn" layoutX="498.0" layoutY="239.0" mnemonicParsing="false" onAction="#onUpdateAppClick" prefHeight="27.0" prefWidth="81.0" text="UPDATE">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Button>
                <Button fx:id="deleteAppBtn" layoutX="602.0" layoutY="239.0" mnemonicParsing="false" onAction="#onDeleteAppClick" prefHeight="27.0" prefWidth="81.0" text="DELETE">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Button>
                <Button fx:id="backBtn" layoutX="815.0" layoutY="270.0" mnemonicParsing="false" onAction="#onBackClick" text="Back">
                    <font>
                        <Font name="Leelawadee UI" size="11.0" />
                    </font>
                </Button>
            <RadioButton fx:id="monthAppRadio" layoutX="158.0" layoutY="254.0" mnemonicParsing="false" onAction="#onMonthAppClick" text="Month" />
            <RadioButton fx:id="weekAppRadio" layoutX="158.0" layoutY="231.0" mnemonicParsing="false" onAction="#onWeekAppClick" text="Week" />
            <Label layoutX="110.0" layoutY="247.0" text="Filters:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <RadioButton fx:id="allAppRadio" layoutX="158.0" layoutY="277.0" mnemonicParsing="false" onAction="#onAllAppClick" selected="true" text="All" />
            </children>
        </AnchorPane>
        <Label layoutX="343.0" layoutY="14.0" text="Appointments" textFill="#eeeeee">
            <font>
                <Font name="Leelawadee UI Bold" size="36.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
