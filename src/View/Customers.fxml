<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="702.0" style="-fx-background-color: #59757a;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d9d9d900" height="370.0" layoutX="16.0" layoutY="14.0" stroke="#eeeeee" strokeType="INSIDE" width="669.0" />
      <AnchorPane layoutX="30.0" layoutY="69.0" prefHeight="303.0" prefWidth="540.0" style="-fx-background-color: #eeeeee;">
         <children>
            <TableView fx:id="customerTable" prefHeight="222.0" prefWidth="640.0">
              <columns>
                <TableColumn fx:id="custIdCol" prefWidth="55.0" text="ID" />
                <TableColumn fx:id="custNameCol" prefWidth="136.0" text="Name" />
                  <TableColumn fx:id="custAddressCol" prefWidth="192.0" text="Address" />
                  <TableColumn fx:id="custPostalCol" prefWidth="77.0" text="Postal Code" />
                  <TableColumn fx:id="custDivisionCol" prefWidth="70.0" text="Division" />
                  <TableColumn fx:id="custPhoneCol" prefWidth="109.0" text="Phone" />
              </columns>
            </TableView>
            <Button fx:id="newCustomerBtn" layoutX="176.0" layoutY="239.0" mnemonicParsing="false" onAction="#onNewCustomerClick" prefHeight="27.0" prefWidth="81.0" text="NEW">
               <font>
                  <Font name="Leelawadee UI" size="15.0" />
               </font>
            </Button>
            <Button fx:id="updateCustomerBtn" layoutX="280.0" layoutY="239.0" mnemonicParsing="false" onAction="#onUpdateCustomerClick" prefHeight="27.0" prefWidth="81.0" text="UPDATE">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="deleteCustomerBtn" layoutX="384.0" layoutY="239.0" mnemonicParsing="false" onAction="#onDeleteCustomerClick" prefHeight="27.0" prefWidth="81.0" text="DELETE">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="backBtn" layoutX="581.0" layoutY="270.0" mnemonicParsing="false" onAction="#onBackClick" text="Back">
               <font>
                  <Font name="Leelawadee UI" size="11.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Label layoutX="261.0" layoutY="14.0" text="Customers" textFill="#eeeeee">
         <font>
            <Font name="Leelawadee UI Bold" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
